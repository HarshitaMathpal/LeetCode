class MyHashSet {
    int a[];
    public MyHashSet() {
        a=new int[1000001];
    }
    
    public void add(int key) {
        
        a[key]=1;
    }
    
    public void remove(int key) {
        a[key]=0;
    }
    public boolean contains(int key){
        if(a[key]==0)
            return false;
        return true;
    }
    
}
